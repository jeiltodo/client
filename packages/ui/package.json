{
  "name": "@jeiltodo/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    ".": "./src/shared/index.ts",
    "./asset/index.ts": "./src/shared/asset/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "lint": "eslint src/",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@jeiltodo/eslint-config": "*",
    "@jeiltodo/tailwind-config": "*",
    "@jeiltodo/typescript-config": "*",
    "@jeiltodo/icons": "*",
    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@jeiltodo/icons": "*"
  }
}
